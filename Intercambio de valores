#include<stdio.h>
using namespace std;

void main()
{
	int A, B;
	cout << "Valor de A: ";
	cin >> A;
	cout << "Valor de B: ";
	cin >> B;
	int aux1 = A;
	A = B;
	B = aux1;
	cout << "Intercambio de valores_ejecutad0" << endl;
	cout << "A: " << A << endl;
	cout << "B: " << B << endl;


	


	_getwch();
}


//Pseudocodigo//

Proceso Intercambio
	Escribir "valor A: "
	Leer A
	Escribir "valor B: "
	Leer B
	aux<-A
	A<-B
	B<-AUX
	Escribir "Imtercambio_de_valores_ejecutado"
	Escribir "El valor de A es: ",A
	Escribir "El valor de B es: ",B
FinProceso
